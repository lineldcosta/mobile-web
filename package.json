{
  "name": "isomorphic",
  "version": "0.5.0",
  "description": "A React boilerplate offering server-side rendering, code-splitting, hot reloading and more",
  "main": "webserver/server.production.js",
  "author": "Viral Ganatra",
  "license": "MIT",
  "dependencies": {
    "babel-core": "7.0.0-bridge.0",
    "express": "^4.16.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-art": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-native": "^0.57.5",
    "react-native-web": "^0.9.8",
    "react-redux": "^5.1.1",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-injector": "^0.1.0",
    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@viralganatra/app-scripts": "^1.0.1",
    "autoprefixer": "^8.5.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "cluster": "^0.7.7",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.11",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "file-loader": "^1.1.6",
    "hbs": "^4.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lru-cache": "^4.1.3",
    "metro-react-native-babel-preset": "0.49.2",
    "mini-css-extract-plugin": "^0.4.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "os": "^0.1.1",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.5",
    "react-hot-loader": "^4.1.2",
    "react-test-renderer": "^16.6.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "shrink-ray": "^0.1.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^1.0.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  },
  "scripts": {
    "start:dev": "npm run webpack:dev && npm run node:dev",
    "webpack:dev": "npm run clean:dev && npm run webpack:client:dev && npm run webpack:server:dev",
    "node:dev": "node --inspect webserver/server.development.js",
    "clean:dev": "rimraf webclient/development && rimraf webserver/development && mkdirp webclient/development && mkdirp webserver/development",
    "webpack:client:dev": "webpack --config config/webpack/client.config.js --profile --json > webclient/development/client.development.stats.webpack.json --env.dev",
    "webpack:server:dev": "webpack --config config/webpack/server.config.js --profile --json > webserver/development/server.development.stats.webpack.json --env.dev",
    "start:prod": "npm run webpack:prod && npm run node:prod",
    "webpack:prod": "npm run clean:prod && npm run webpack:client:prod && npm run webpack:server:prod",
    "node:prod": "node webserver/server.production.js",
    "clean:prod": "rimraf webclient/production && rimraf webserver/production && mkdirp webclient/production && mkdirp webserver/production",
    "webpack:client:prod": "webpack --config config/webpack/client.config.js --profile --json > webclient/production/client.production.stats.webpack.json --env.prod",
    "webpack:server:prod": "webpack --color true --progress --config config/webpack/server.config.js --profile --json > webserver/production/server.production.stats.webpack.json --env.prod",
    "start:hmr": "node ./webserver/server.hmr.js",
    "start:web": "node ./webnative/server.hmr.js",
    "start-android": "npm run clear-cache && react-native run-android",
    "start-ios": "npm run clear-cache && react-native run-ios",
    "lint": "app-scripts lint",
    "precommit": "app-scripts precommit",
    "clear-cache": "node webserver/npm-clear-cache.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleDirectories": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleNameMapper": {
      "^application(.*)$": "<rootDir>/src/application$1",
      "^services(.*)$": "<rootDir>/src/application/services$1",
      "^shared(.*)$": "<rootDir>/src/application/shared$1",
      "^utils(.*)$": "<rootDir>/src/application/utils$1",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/config/test/jest.transform.js"
    },
    "verbose": true
  }
}
